{
  "name": "@cara/demo-app",
  "description": "A web app built with koa and porter",
  "version": "4.0.0-beta.9",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "koa": "^2.4.1",
    "koa-static": "^4.0.2"
  },
  "scripts": {
    "build": "rm -rf public && node bin/build.js",
    "dev": "rm -rf public && DEBUG=porter,$DEBUG node-dev app.js",
    "start": "rm -rf public && DEBUG=porter,$DEBUG porter serve --paths components --paths browser_modules --lazyload mad-import/foo.js",
    "test": "rm -rf public && DEBUG=porter,$DEBUG porter serve --paths components --paths browser_modules --lazyload mad-import/foo.js --headless"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.16.3",
    "@cara/demo-wasm": "^4.0.0-beta.9",
    "@cara/demo-worker": "^4.0.0-beta.9",
    "@cara/porter": "^4.0.0-beta.9",
    "@cara/porter-cli": "^4.0.0-beta.9",
    "brfs": "^2.0.1",
    "brotli": "^1.3.2",
    "buffer": "^5.2.1",
    "chart.js": "^2.7.3",
    "cropper": "^3.0.0",
    "events": "^3.0.0",
    "expect.js": "^0.3.1",
    "glob": "^7.1.2",
    "heredoc": "^1.3.1",
    "iconv-lite": "^0.4.24",
    "inferno": "^3.9.0",
    "jquery": "^3.3.1",
    "mobx": "^6.3.13",
    "mocha": "^9.1.1",
    "node-dev": "^3.1.3",
    "nyc": "^15.1.0",
    "object-inspect": "^1.11.1",
    "path": "^0.12.7",
    "prismjs": "^1.7.0",
    "react": "^16.2.0",
    "react-color": "^2.13.8",
    "react-datepicker": "^1.0.4",
    "react-dom": "^16.2.0",
    "react-stack-grid": "^0.7.1",
    "readable-stream": "^3.1.0",
    "yen": "1.2.4"
  },
  "browserify": {
    "transform": [
      "envify",
      "brfs"
    ]
  }
}
